<!--CONTENT-->
<section id="content"
         class="pt-42px pb-36px pl-ls-42px overflow-ls-hidden overflow-md-hidden pl-ls-lg-0 pt-xl-66px pb-lg-66px mw-1440px mx-xl-auto w-100">

    <!--NEWS-->
    <div class="bg-white pt-24px px-24px position-relative d-md-flex px-xl-48px mx-xl-n30px justify-content-md-center flex-column">

        <a href="/"
           class="button lvl3 align-self-ls-md-start align-self-md-start position-relative right-0 right-hover-12px fs-short-2"><i
                    class="fas fa-angle-left pr-8px d-ls-md-inline pr-md-12px fs-short-2 d-md-inline"></i>Retour Ã 
            l'accueil</a>

        <!--title-->
        <h2 class="text-ls-md-left pt-ls-md-8px text-md-left pt-md-8px">Agenda des manifestations</h2>

        <!--events-->
        <div class="px-0 d-md-flex flex-md-column d-ls-md-flex flex-ls-md-column pt-48px align-items-center">

            <ul class="pt-24px pt-md-32px d-md-flex flex-md-wrap mx-md-n4px mx-lg-n8px object-cardsList">
                <li class="object-card oc-event col-md-6 px-md-4px col-lg-4 px-lg-8px">
                    <a href="#" class="bg-img">
                        Erreur de chargement de l'agenda
                    </a>
                </li>
            </ul>
            <noscript>
                <ul class="pt-24px pt-md-32px d-md-flex flex-md-wrap mx-md-n4px mx-lg-n8px object-cardsList">
                    {% if events|length >0 %}
                        {% include 'agenda/list/_first_lines.html.twig' %}
                        {% include 'agenda/list/_other_lines.html.twig' %}
                    {% else %}
                        <li class="object-card oc-event col-md-6 px-md-4px col-lg-4 px-lg-8px">
                            <a href="#" class="bg-img">
                                Erreur de chargement
                            </a>
                        </li>
                    {% endif %}
                </ul>
            </noscript>
            <div id="app-agenda"></div>
        </div>
    </div>
