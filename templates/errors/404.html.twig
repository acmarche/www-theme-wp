{% extends "article/base.html.twig" %}

{% block content %}
    <p>Cette page n'existe plus</p>
    {% if result is defined %}
        <p><strong>Voici quelques suggestions émises par le moteur de recherche:</strong></p>
        {{ result|raw }}
    {% endif %}
    <a href="/">
        <p>Retour à l'accueil</p>
    </a>
{% endblock %}

{% block image %}
    <div class="bg-img w-100 h-220px position-relative h-lg-330px">
        <i style="background-image: url('{{ template_directory }}/assets/images/500.jpg')"
           class="bg-img-news-1 position-absolute top-0 bottom-0 left-0 right-0 md-left-15px md-right-15px lg-left-0 lg-right-0"></i>
    </div>
{% endblock %}

{% block recommandation %}

{% endblock %}
